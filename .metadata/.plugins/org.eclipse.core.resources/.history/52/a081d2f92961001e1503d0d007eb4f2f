package inheritance;

class Cals {
	// ´õÇÏ±â, »©±â
	int add(int num1, int num2) {
		return num1 + num2;
	}
	
	int minus(int num1, int num2) {
		return num1 - num2;
	}
}

class AdvCals extends Cals {
	// ´õÇÏ±â, »©±â, °öÇÏ±â, ³ª´©±â
	
	int mul(int num1, int num2) {
		return num1 * num2;
	}
	
	int div(int num1, int num2) {
		return num1 / num2;
	}
}

public class InherEx02 {

	public static void main(String[] args) {
		Cals cal = new Cals();

	}

}
