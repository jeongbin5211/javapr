	public static void main(String[] args) {
		// 사원가입(이미지), 게시판(이미지, 첨부파일) : 오늘 첨부파일
		// 입사일 : 년월일 폴더
		
		// 파일 생성
		// 경로 구분자(seperator) : /
		// window는 / 사용
		// 백슬래쉬 쓰고싶으면 \\ \두번입력
		
		String file = "c:/temp/korea.txt";
		
		try {
			// 구현 코드
		} catch (Exception e) {// 여러 개 작성 가능
			
			throw e; // 최상위 클래스가 아니라면 무조건 던져주기
			// 예외처리
		} finally {
			// 항상 처리되는 코드
			
			
		}

	}

}
