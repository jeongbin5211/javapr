package abstractEx;

interface Boot {
	void powerOn();
	void powerOff();
}

interface System {
	void msOS();
	void appleOS();
}

class Mac implements Boot, System {
	
}



public class interfaceEx {

	public static void main(String[] args) {
		// 부팅, 운영체제 따로 만들어서 다중 상속 처리
		// abstract 상속 - 오버라이드
		
		// interface -> implements : 상속을 받아 구현

	}

}
